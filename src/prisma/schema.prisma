generator client {
    provider = "prisma-client-js"
    //previewFeatures = ["fulltextSearch", "fullTextIndex"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model TimesheetItem {
    id       String   @id @default(uuid())
    userId   String
    name     String
    timeFrom DateTime
    timeTo   DateTime

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
